{
  "name": "cherry-picked-mobile",
  "version": "1.0.0",
  "description": "React Native mobile app for Cherry Picked - Personalized fashion sale discovery platform",
  "main": "expo/AppEntry.js",
  "private": true,
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-push": "^3.6.7"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
